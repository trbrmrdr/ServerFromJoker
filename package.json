    "@types/mongodb": "^3.6.17",
    "@types/node": "^14.17.2",
    "@types/pino": "^6.3.8",
    "@types/ws": "^7.4.4",
    "concurrently": "^6.2.0",
    "fastify-tsconfig": "^1.0.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testTimeout": 20000,
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
