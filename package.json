{
  "name": "jocker-be",
  "version": "1.0.0",
  "description": "## How to use ?",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --runInBand",
    "start": "npm run build && fastify start -l info dist/app.js",
    "build": "tsc",
    "start:ts": "ts-node src/index.ts",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"fastify start -w -l info -P dist/app.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.4.4",
    "@sinclair/typebox": "^0.16.5",
    "agenda": "^4.1.3",
    "ajv": "^8.1.0",
    "ajv-formats": "^2.1.0",
    "ajv-keywords": "^5.0.0",
    "close-with-grace": "^1.1.0",
    "date-fns": "^2.22.1",
    "fastify": "^3.17.0",
    "fastify-autoload": "^3.7.1",
    "fastify-cli": "^2.13.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fastify-static": "^4.2.2",
    "fastify-swagger": "^4.7.0",
    "ioredis": "^4.27.6",
    "mongodb": "^3.6.9",
    "pino": "^6.11.3",
    "pino-pretty": "^4.8.0",
    "ws": "^7.4.6",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/agenda": "^4.1.0",
    "@types/ioredis": "^4.26.4",
